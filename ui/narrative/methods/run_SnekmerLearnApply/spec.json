{
    "ver": "0.0.1",
    "authors": [
        "jjacobson"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "input_seqs",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseSequences.ProteinSequenceSet"] 
            }
        },
        {
            "id": "kmer_length",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 0
            }
        },
        {
            "id": "alphabet",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "str",
                "min_integer" : 0
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"SnekmerLearnApply",
            "method": "run_SnekmerLearnApply",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "input_seqs",
                    "target_property": "input_seqs",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "kmer_length",
                    "target_property": "kmer_length"
                },{
                "input_parameter": "alphabet",
                "target_property": "alphabet"
                }              
                ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
