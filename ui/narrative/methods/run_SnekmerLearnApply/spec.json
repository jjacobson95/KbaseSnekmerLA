{
    "ver": "0.0.1",
    "authors": [
        "jjacobson"
    ],
    "contact": "https://kbase.us/contact-us/",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "input_type",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["protein"],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "protein",
                        "display": "Protein Sequence Set"
                    },
                    {
                        "value": "genome",
                        "display": "Genome"
                    }
                ]
            }
        },
        {
            "id": "protein_input",
            "optional": true,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [""],
            "field_type": "text",
            "ui_name": "Protein Input",
            "short_hint": "Input for Protein Sequence Set.",
            "text_options": {
                "valid_ws_types": ["KBaseSequences.ProteinSequenceSet"]
            }
        },
        {
            "id": "genome_input",
            "optional": true,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [""],
            "field_type": "text",
            "ui_name": "Genome Input",
            "short_hint": "Input for Genome.",
            "text_options": {
                "valid_ws_types": ["KBaseGenomes.Genome"]
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name":"SnekmerLearnApply",
            "method": "run_SnekmerLearnApply",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "protein_input",
                    "target_property": "protein_input",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "genome_input",
                    "target_property": "genome_input",
                    "target_type_transform": "resolved-ref"
                }         
                ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
